{
  "name": "nextjs-with-typescript",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:static": "next build && next export",
    "serve:static": "npx serve out",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "i18n": "npm run extract && npm run compile",
    "extract": "NODE_ENV=development lingui extract",
    "compile": "lingui compile",
    "pre-commit": "lint-staged && npm run i18n && git add ./src/translations",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "gql-gen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@aave/contract-helpers": "0.28.2",
    "@aave/math-utils": "0.28.2",
    "@apollo/client": "^3.5.7",
    "@bumped-inc/storybook-addon-lingui-v3": "^0.0.4",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.4",
    "@heroicons/react": "^1.0.5",
    "@lingui/react": "^3.13.1",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@toruslabs/torus-embed": "^1.19.0",
    "@walletconnect/web3-provider": "^1.7.1",
    "dayjs": "^1.10.7",
    "eth-provider": "^0.10.0",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^5.5.3",
    "graphql": "^16.2.0",
    "graphql-ws": "^5.5.5",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "reflect-metadata": "^0.1.13",
    "subscriptions-transport-ws": "^0.11.0",
    "walletlink": "^2.4.5",
    "web3modal": "^1.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@lingui/cli": "^3.13.1",
    "@lingui/macro": "^3.13.1",
    "@storybook/addon-actions": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/builder-webpack5": "^6.4.13",
    "@storybook/manager-webpack5": "^6.4.13",
    "@storybook/react": "^6.4.13",
    "@svgr/webpack": "^6.2.0",
    "@types/react": "latest",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.5.1",
    "storybook-addon-next-router": "^3.1.1",
    "typescript": "latest",
    "webpack": "^5.66.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.jsx": "eslint --cache --fix",
    "*.ts": "eslint --cache --fix",
    "*.tsx": "eslint --cache --fix"
  },
  "nextBundleAnalysis": {
    "budget": null,
    "budgetPercentIncreaseRed": 20,
    "showDetails": true
  }
}
