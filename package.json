{
  "name": "nextjs-with-typescript",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true yarn build",
    "build:static": "next build && next export",
    "build:optimize-svgs": "find public -type f -name '*.svg' -print0 | xargs -0 -n 1 -P 6 npx svgo",
    "build:populate-cache": "npx esbuild src/static-build/populateCache.ts --tsconfig=./tsconfig.json --bundle --outfile=scripts/populate-cache.js --platform=node --target=es6",
    "cache:update": "yarn build:populate-cache && CACHE_PATH=../src/static-build node scripts/populate-cache.js",
    "serve:static": "npx serve out",
    "export": "next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "post-update": "echo \"codesandbox preview only, need an update\" && yarn upgrade --latest",
    "i18n": "npm run extract && npm run compile",
    "extract": "NODE_ENV=development lingui extract",
    "compile": "lingui compile",
    "pre-commit": "lint-staged && npm run i18n && git add ./src/translations",
    "prepare": "husky install",
    "gql-gen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@aave/contract-helpers": "0.30.1-8f5f4f7ce43a56df787288d25801c3a383b168b6.0+8f5f4f7",
    "@aave/math-utils": "0.30.1-8f5f4f7ce43a56df787288d25801c3a383b168b6.0+8f5f4f7",
    "@apollo/client": "^3.5.9",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript-operations": "^2.3.2",
    "@graphql-codegen/typescript-react-apollo": "^3.2.8",
    "@heroicons/react": "^1.0.5",
    "@lingui/react": "^3.13.2",
    "@mui/icons-material": "^5.4.2",
    "@mui/material": "^5.4.2",
    "@toruslabs/torus-embed": "^1.20.4",
    "@types/d3-array": "^3.0.2",
    "@types/d3-time-format": "^4.0.0",
    "@visx/axis": "^2.6.0",
    "@visx/curve": "^2.1.0",
    "@visx/event": "^2.6.0",
    "@visx/gradient": "^2.1.0",
    "@visx/grid": "^2.6.0",
    "@visx/legend": "^2.2.2",
    "@visx/mock-data": "^2.1.2",
    "@visx/responsive": "^2.8.0",
    "@visx/scale": "^2.2.2",
    "@visx/shape": "^2.4.0",
    "@visx/tooltip": "^2.8.0",
    "@walletconnect/web3-provider": "^1.7.1",
    "d3-array": "^3.1.1",
    "d3-time-format": "^4.1.0",
    "dayjs": "^1.10.7",
    "eth-provider": "^0.10.0",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^5.5.4",
    "graphql": "^16.2.0",
    "graphql-ws": "^5.6.0",
    "lowdb": "^3.0.0",
    "next": "12.1.0",
    "react": "latest",
    "react-dom": "latest",
    "react-markdown": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "remark-gfm": "^3.0.1",
    "subscriptions-transport-ws": "^0.11.0",
    "walletlink": "^2.4.7",
    "web3modal": "^1.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@lingui/cli": "^3.13.2",
    "@lingui/macro": "^3.13.2",
    "@next/bundle-analyzer": "^12.1.0",
    "@svgr/webpack": "^6.2.1",
    "@types/lodash": "^4.14.178",
    "@types/react": "latest",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": ">=6",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "contributors": [
    "Pol Sendra <pol@aave.com>",
    "Lukas Strassel <lukas@aave.com>",
    "Oleksandr Tkach <oleksandr@aave.com>",
    "David Racero <david.k@aave.com>",
    "Andrew Schmidt <andrew@aave.com>",
    "Mark Hinschberger <mark@aave.com>",
    "Nikita Bokarev <nikita@aave.com>",
    "Anastasia Khovaeva <anastasia@aave.com>",
    "Diego Prudencio diego@aave.com"
  ],
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.jsx": "eslint --cache --fix",
    "*.ts": "eslint --cache --fix",
    "*.tsx": "eslint --cache --fix"
  },
  "nextBundleAnalysis": {
    "budget": null,
    "budgetPercentIncreaseRed": 20,
    "showDetails": true
  }
}
